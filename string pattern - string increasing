import java.util.*;
public class Main{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        System.out.print("Enter a string:");
        String a=sc.nextLine();
        int n=a.length();
        int st=1;
        for(int i=1;i<=n;i++){
            for(int j=1;j<=st;j++){
                System.out.print(a.charAt(j-1)+" ");
            }
            System.out.println();
            st++;
        }
    }
}

